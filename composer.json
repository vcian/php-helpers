{
    "name": "vcian/php-helpers",
    "description": "PHP Helpers.",
    "keywords": [
        "Helpers",
        "Php"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vcian - ViitorCloud",
            "homepage": "https://github.com/vcian"
        }
    ],
    "require": {
        "php": "^8.1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5.1",
        "guzzlehttp/psr7": "^2.5.0",
        "laravel/pint": "^1.9.0",
        "pestphp/pest": "^2.5.3",
        "pestphp/pest-plugin-arch": "^2.1.2",
        "pestphp/pest-plugin-mock": "^2.0.0",
        "phpstan/phpstan": "^1.10.14",
        "rector/rector": "^0.14.8",
        "symfony/var-dumper": "^6.2.8"
    },
    "autoload": {
        "psr-4": {
            "Vcian\\PHPHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
